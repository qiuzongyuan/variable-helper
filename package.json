{
  "name": "variable-helper",
  "displayName": "Variable Helper",
  "description": "%description%",
  "icon": "images/icon.png",
  "publisher": "ayuan",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "vscode",
    "plugin",
    "format",
    "格式化"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiuzongyuan/variable-helper"
  },
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "camelCase",
        "title": "camelCase"
      },
      {
        "command": "snakeCase",
        "title": "snake_case"
      },
      {
        "command": "kebabCase",
        "title": "kebab-case"
      },
      {
        "command": "pascalCase",
        "title": "PascalCase"
      },
      {
        "command": "screamingSnakeCase",
        "title": "SCREAMING_SNAKE_CASE"
      },
      {
        "command": "capitalizedSnakeCase",
        "title": "Capitalized_Snake_Case"
      },
      {
        "command": "sentenceCase",
        "title": "Sentence Case"
      },
      {
        "command": "lowerCase",
        "title": "lower case"
      },
      {
        "command": "upperCase",
        "title": "UPPER CASE"
      }
    ],
    "submenus": [
      {
        "id": "formatVariable",
        "label": "%formatVariable%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "formatVariable",
          "group": "1_modification",
          "when": "editorHasSelection"
        }
      ],
      "formatVariable": [
        {
          "command": "camelCase",
          "group": "1_modification@1"
        },
        {
          "command": "pascalCase",
          "group": "1_modification@2"
        },
        {
          "command": "kebabCase",
          "group": "1_modification@3"
        },
        {
          "command": "snakeCase",
          "group": "1_modification@4"
        },
        {
          "command": "screamingSnakeCase",
          "group": "1_modification@5"
        },
        {
          "command": "capitalizedSnakeCase",
          "group": "1_modification@6"
        },
        {
          "command": "sentenceCase",
          "group": "1_modification@7"
        },
        {
          "command": "lowerCase",
          "group": "1_modification@8"
        },
        {
          "command": "upperCase",
          "group": "1_modification@9"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish --no-yarn"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
  }
}
