{
  "name": "variable-helper",
  "displayName": "Variable Helper",
  "publisher": "ayuan",
  "icon": "images/icon.png",
  "description": "",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "plugin"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/qiuzongyuan/repository.git"
  },
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "camelCase",
        "title": "camelCase"
      },
      {
        "command": "snakeCase",
        "title": "snake_case"
      },
      {
        "command": "kebabCase",
        "title": "kebab-case"
      },
      {
        "command": "pascalCase",
        "title": "PascalCase"
      },
      {
        "command": "screamingSnakeCase",
        "title": "SCREAMING_SNAKE_CASE"
      },
      {
        "command": "capitalizedSnakeCase",
        "title": "Capitalized_Snake_Case"
      },
      {
        "command": "spaceLowerCase",
        "title": "space lower case"
      },
      {
        "command": "spaceUpperCase",
        "title": "SPACE UPPER CASE"
      }
    ],
    "submenus": [
      {
        "id": "variableHelper",
        "label": "Variable Helper"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "variableHelper",
          "group": "1_modification",
          "when": "editorHasSelection"
        }
      ],
      "variableHelper": [
        {
          "command": "camelCase",
          "group": "1_modification"
        },
        {
          "command": "snakeCase",
          "group": "1_modification"
        },
        {
          "command": "kebabCase",
          "group": "1_modification"
        },
        {
          "command": "pascalCase",
          "group": "1_modification"
        },
        {
          "command": "screamingSnakeCase",
          "group": "1_modification"
        },
        {
          "command": "capitalizedSnakeCase",
          "group": "1_modification"
        },
        {
          "command": "spaceCamelCase",
          "group": "1_modification"
        },
        {
          "command": "spaceLowerCase",
          "group": "1_modification"
        },
        {
          "command": "spaceUpperCase",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish --no-yarn"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
